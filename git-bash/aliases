# git
alias g=git
alias gin='git init'
alias gs='git status'
alias gsb='git status -sb'
alias clone='git clone'
alias gb='git branch'
alias gc='git commit'
alias gcmsg='git commit -m'
alias gcam='git commit -a -m'
alias gcb='git checkout -b'
alias gcf='git config --list'
alias gco='git checkout'
alias gd='git diff'
alias gf='git fetch'
alias gpull='git pull origin "$(git_current_branch)"'
alias gpush='git push origin "$(git_current_branch)"'
alias ghh='git help'
alias gpl='git pull'
alias gpsh='git push'
alias gl='git log'
alias glgg='git log --graph'
alias gr='git remote'
alias gra='git remote add'
alias grmv='git remote rename'
alias grrm='git remote remove origin'
alias grurl='git config --get remote.origin.url'
alias glc='git config --local --list'
alias ggr='git config'
alias grset='git remote set-url'
alias grb='git rebase'
alias grba='git rebase --abort'
alias grev='git revert'
alias grh='git reset'
alias grhh='git reset --hard'
alias grm='git rm'
alias grmc='git rm --cached'
# system
alias md=mkdir
alias rd=rmdir
alias la='ls -a'
# php composer - laravel
alias art='php artisan'
alias serve='php artisan serve'
alias tinker='php artisan tinker'
alias migrate='php artisan migrate'
alias fresh='php artisan migrate:fresh --seed'
alias cclear='php artisan cache:clear'